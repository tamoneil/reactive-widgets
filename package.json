{
  "name": "react-widgets",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline",
    "hot-dev-server": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "watch-prerender": "webpack --watch --config webpack-dev-prerender.config --progress --profile --colors",
    "server": "pm2 ./build/server/server.js",
    "test": "karma start",
    "lint": "eslint ./src/"
  },
  "description": "Project template for building isomorphic widgets using reactjs",
  "main": "index.js",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.12.0",
    "eslint": "^0.20.0",
    "eslint-plugin-react": "^2.2.0",
    "extract-text-webpack-plugin": "^0.7.0",
    "file-loader": "^0.8.1",
    "html-loader": "^0.3.0",
    "jasmine-core": "^2.3.0",
    "jsx-loader": "^0.13.2",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.8",
    "karma-cli": "0.0.4",
    "karma-jasmine": "^0.3.5",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-webpack": "^1.5.1",
    "null-loader": "^0.1.1",
    "react-hot-loader": "^1.2.6",
    "react-proxy-loader": "^0.3.4",
    "source-map-support": "^0.2.10",
    "style-loader": "^0.12.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.11",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "babel-core": "^5.2.12",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.2.12",
    "body-parser": "^1.12.3",
    "classnames": "^2.0.0",
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "lodash": "^3.8.0",
    "morgan": "^1.5.2",
    "react": "^0.13.2",
    "rx": "^2.5.2",
    "superagent": "^1.2.0"
  }
